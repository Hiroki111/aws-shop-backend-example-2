{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Product Service created API Gateway and Lambdas",
  "main": "handler.js",
  "scripts": {
    "package-build": "sls package",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:fileParser": "sls deploy function -f importFileParser --stage dev",
    "deploy:import": "sls deploy function -f importProductsFile --stage dev",
    "deploy:prod": "sls deploy --stage prod",
    "remove:dev-stack": "sls remove --stage dev --region eu-west-1",
    "remove:prod-stack": "sls remove --stage prod --region eu-west-1",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --coverage"
  },
  "devDependencies": {
    "aws-sdk": "^2.1183.0",
    "@aws-sdk/types": "3.127.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^28.1.6",
    "@types/node": "^10.17.56",
    "@types/serverless": "^3.12.7",
    "fork-ts-checker-webpack-plugin": "^7.2.12",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-webpack": "^5.7.1",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "JSCC",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.137.0",
    "@aws-sdk/s3-request-presigner": "^3.137.0",
    "csv-parser": "^3.0.0",
    "winston": "^3.3.3"
  }
}
